function AliZip1($AliZip9, $AliZip10 = 2){
$AliZip11=array(' B', ' KB', ' MB', ' GB', ' TB', ' PB', ' EB', ' ZB', ' YB');
$AliZip12=floor((strlen($AliZip9) - 1) / 3);
return sprintf("%.{$AliZip10}f", $AliZip9 / pow(1024, $AliZip12)) . @$AliZip11[$AliZip12];
}
